{
  "name": "react-elysium",
  "version": "0.1.0",
  "private": false,
  "license": "MIT",
  "dependencies": {
    "axios": "^0.18.0",
    "bulma": "0.7.2",
    "classnames": "^2.2.6",
    "react": "16.6.3",
    "react-dom": "16.6.3",
    "react-helmet": "^5.2.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.1.1",
    "recharts": "1.4.1",
    "validator": "10.9.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "lint": "prettier --trailing-comma es5 --single-quote --write '*/*.js' '*/!(node_modules)/**/*.js'",
    "test": "react-scripts test --env=jsdom",
    "coverage": "yarn test --coverage",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "enzyme": "3.7.0",
    "enzyme-adapter-react-16": "1.7.0",
    "husky": "^1.2.0",
    "immutability-helper": "^2.7.1",
    "lint-staged": "^8.1.0",
    "npm-run-all": "4.1.5",
    "prettier": "1.15.3",
    "react-testing-library": "5.3.0",
    "standard": "^12.0.1",
    "stylelint": "9.9.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-scss": "3.4.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,md,css,html}": [
      "prettier --trailing-comma es5 --single-quote --write",
      "git add"
    ],
    "yarn.lock": [
      "git rm --cached"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
